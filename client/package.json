    "rimraf": "^2.5.4",
    "sass-loader": "^4.0.0",
    "serialize-javascript": "^1.1.2",
    "serve-favicon": "^2.3.0",
    "serve-static": "^1.10.0",
    "strip-loader": "^0.1.0",
    "style-loader": "^0.13.0",
    "universal-webpack": "^0.1.40",
    "uri-js": "^2.1.1",
    "url-loader": "^0.5.6",
    "uuid": "https://registry.npmjs.org/uuid/-/uuid-2.0.2.tgz",
    "webfonts-generator": "^0.3.2",
    "webpack": "^1.12.2",
    "webpack-dev-server": "^1.14.1",
    "webpack-hot-middleware": "^2.4.1"
  },
  "devDependencies": {
    "babel-jest": "^16.0.0",
    "enzyme": "^2.3.0",
