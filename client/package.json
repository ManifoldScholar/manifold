{
  "name": "manifold-client",
  "description": "The Manifold Scholarship Client",
  "author": "Zach Davis <zach@castironcoding.com>",
  "license": "GPL-3.0",
  "version": "0.0.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/manifoldScholar/manifold"
  },
  "keywords": [],
  "main": "bin/server.js",
  "scripts": {
    "build": "npm-run-all clean build:**",
    "build:client": "NODE_ENV=production webpack --config ./webpack/build-client.config.js --colors --optimize-minimize",
    "build:servers": "NODE_ENV=production webpack --config ./webpack/build-servers.config.js --colors",
    "clean": "rimraf -r dist",
    "serve": "npm-run-all --parallel serve:**",
    "serve:web": "NODE_ENV=production node dist/node/web.js",
    "serve:renderer": "NODE_ENV=production node dist/node/renderer.js",
    "dev": "npm-run-all clean --parallel dev:build:** dev:serve:**",
    "dev:build:servers": "NODE_ENV=development WEBPACK_DEV_SERVER=true webpack --config ./webpack/build-servers.config.js --colors",
    "dev:serve:client": "NODE_ENV=development WEBPACK_DEV_SERVER=true node webpack/servers/dev.js",
    "dev:serve:web": "npm-run-all wait:web dev:start:web",
    "dev:serve:renderer": "npm-run-all wait:renderer dev:start:renderer",
    "dev:start:web": "NODE_ENV=development WEBPACK_DEV_SERVER=true node dist/node/web.js",
    "dev:start:renderer": "NODE_ENV=development  nodemon -q dist/node/renderer.js --watch dist/node/renderer.js",
    "wait:web": "node script/wait.js --name=\"Web\" --on=dist/node/web.js,dist/manifest/client.json",
    "wait:renderer": "node script/wait.js --name=\"Renderer\" --on=dist/node/renderer.js,dist/manifest/client.json",
    "lint": "eslint .",
    "test": "jest --verbose",
    "test-ci": "jest --runInBand"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json"
    ],
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "setupFiles": [
      "./src/test/setup.js"
    ],
    "globals": {
      "__CLIENT__": false,
      "__SERVER__": true
    }
  },
  "dependencies": {
