    "compression": "^1.6.0",
    "connect": "^3.6.6",
    "cookie": "^0.3.1",
    "date-fns": "^1.29.0",
    "dotenv": "^6.0",
    "entities": "^1.1.1",
    "es6-promise": "^4.0.5",
    "express-session": "^1.12.1",
    "filesize": "^3.3.0",
    "focus-trap-react": "^3.1.2",
    "glob": "^7.0.0",
    "he": "^1.2.0",
    "hoist-non-react-statics": "^2.0.0",
    "http-proxy-middleware": ">=0.17.2",
    "humps": "^2.0.0",
    "immutability-helper": "^2.6.4",
    "isomorphic-fetch": "^2.2.0",
    "lodash": "^4.11.2",
    "minimist": "^1.2.0",
    "morgan": "^1.6.1",
    "nl2br": "^0.0.3",
    "node-emoji": "^1.3.0",
    "node-noop": "^1.0.0",
    "npm-run-all": "^4.0.2",
    "piping": "^1.0.0-rc.1",
    "pretty-error": "^2.0.0",
    "prop-types": "^15.5.10",
    "qs": "^6.0.0",
    "query-string": "5",
    "react": "^16.4",
    "react-ace": "^5.9.0",
