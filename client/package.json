{
  "name": "manifold-client",
  "description": "The Manifold Scholarship Client",
  "author": "Zach Davis <zach@castironcoding.com>",
  "license": "GPL-3.0",
  "version": "0.0.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/manifoldScholar/manifold"
  },
  "keywords": [],
  "main": "bin/server.js",
  "scripts": {
    "start": "NODE_ENV=production npm-run-all prod",
    "start-prod": "NODE_ENV=production npm-run-all prebuild dist prod",
    "start-dev": "npm-run-all --silent prebuild dist:universal --parallel dev",
    "prebuild": "npm-run-all prebuild:**",
    "prebuild:clean": "node ./script/clean-builds-entry.js",
    "dev": "npm-run-all --parallel dev:**",
    "dev:build:universal": "webpack --config ./webpack/entries/react-server.config.entry.js --watch --colors --display-error-details",
    "dev:build:web": "webpack --config ./webpack/entries/web-server.config.entry.js --colors --display-error-details",
    "dev:serve:bundle": "node ./webpack/entries/client.server.entry.js",
    "dev:serve:universal": "nodemon -q ./script/react-server-entry --watch ./dist/build/server/react-server",
    "dev:serve:web": "node ./script/web-server-entry",
    "dist": "npm-run-all --parallel dist:**",
    "dist:client": "webpack --config ./webpack/entries/client.config.entry.js --colors --display-error-details",
    "dist:universal": "webpack --config ./webpack/entries/react-server.config.entry.js --colors --display-error-details",
    "dist:web": "webpack --config ./webpack/entries/web-server.config.entry.js --colors --display-error-details",
    "prod": "npm-run-all --parallel prod:**",
    "prod:serve:web": "node ./script/web-server-entry",
    "prod:serve:universal": "node ./script/react-server-entry",
    "lint": "eslint .",
    "test": "jest --verbose"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json"
    ],
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "globals": {
      "__MANIFOLD_API_URL__": "http://manifold-api.dev:80",
      "__CLIENT__": true,
      "__SERVER__": false,
      "__DEVELOPMENT__": true,
      "__DEVTOOLS__": true
    }
  },
  "dependencies": {
    "autoprefixer": "^6.3.2",
    "babel": "^6.5.2",
    "babel-cli": "^6.16.0",
    "babel-core": "^6.0.0",
