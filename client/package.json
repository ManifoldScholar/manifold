  "name": "manifold-client",
  "description": "The Manifold Scholarship Client",
  "author": "Zach Davis <zach@castironcoding.com>",
  "license": "GPL-3.0",
  "version": "0.0.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/manifoldScholar/manifold"
  },
  "keywords": [],
  "main": "bin/server.js",
  "scripts": {
    "start": "node $NODE_DEBUG_OPTION -r dotenv/config bin/start.js",
    "server": "node -r dotenv/config bin/server.js",
    "webpack": "node -r dotenv/config webpack/webpack-dev-server.js",
    "build": "webpack --verbose --colors --display-error-details --config webpack/prod.config.js",
    "lint": "eslint -c .eslintrc src && scss-lint src/theme/ -c ../.scss-lint.yml",
    "lint-fix": "eslint --fix -c .eslintrc src && scss-lint src/theme/ -c ../.scss-lint.yml",
    "test": "jest --verbose"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json"
    ],
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "globals": {
      "__MANIFOLD_API_URL__": "http://manifold-api.dev:80",
      "__CLIENT__": true,
      "__SERVER__": false,
      "__DEVELOPMENT__": true,
      "__DEVTOOLS__": true
    }
  },
