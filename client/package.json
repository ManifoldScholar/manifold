{
  "name": "manifold-client",
  "description": "The Manifold Scholarship Client",
  "author": "Zach Davis <zach@castironcoding.com>",
  "license": "GPL-3.0",
  "version": "0.0.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/manifoldScholar/manifold"
  },
  "keywords": [],
  "main": "bin/server.js",
  "scripts": {
    "build": "npm-run-all clean build:**",
    "build:servers": "NODE_ENV=production webpack --config ./webpack/build-servers.config.js --colors && rimraf dist/manifold/chunk-*",
    "build:client": "NODE_ENV=production webpack --config ./webpack/build-client.config.js --colors --optimize-minimize",
    "build-tmp": "WEBPACK_BUILD_TARGET=../tmp/ npm-run-all clean-tmp build:**",
    "clean-tmp": "rimraf -r tmp/dist",
    "clean": "rimraf -r dist",
    "serve": "NODE_ENV=production node dist/manifold/server.js",
    "dev": "npm-run-all clean --parallel dev:build:** dev:serve:**",
    "dev:build:servers": "NODE_ENV=development WEBPACK_DEV_SERVER=true webpack --config ./webpack/build-servers.config.js --colors  && rimraf dist/manifold/chunk-*",
    "dev:serve:bundle": "NODE_ENV=development WEBPACK_DEV_SERVER=true node webpack/servers/dev.js",
    "dev:serve:development": "npm-run-all wait:development dev:start:development",
    "dev:serve:client": "npm-run-all wait:client dev:start:client",
    "dev:start:development": "NODE_ENV=development WEBPACK_DEV_SERVER=true node dist/manifold/server-development.js",
    "dev:start:client": "NODE_ENV=development  WEBPACK_DEV_SERVER=true nodemon -q dist/manifold/server.js --watch dist/manifold/server.js",
    "wait:development": "node script/wait.js --name=\"development\" --on=dist/manifold/server-development.js,dist/manifold/manifest.json",
    "wait:client": "node script/wait.js --name=\"client\" --on=dist/manifold/server.js,dist/manifold/manifest.json",
    "lint": "npm-run-all lint:**",
    "lint:js": "echo \"Linting Javascript.\" && eslint .",
    "lint:styles": "echo \"Linting Styles.\" && stylelint \"src/theme/**/*.scss\" --config .stylelintrc",
    "fix-staged": "yarn lint-staged",
    "fix": "npm-run-all fix:**",
    "fix:js": "echo \"Fixing Javascript.\" && eslint --fix .",
    "fix:js:pretty": "echo \"Prettyifying Javascript.\" && prettier --write \"{script,src,webpack}/**/*.js\"",
    "fix:styles": "echo \"Fixing Styles.\" && stylelint \"src/theme/**/*.scss\" --config .stylelintrc --syntax scss --fix",
    "test": "jest --verbose",
    "test-ci": "jest --runInBand"
  },
  "lint-staged": {
    "gitDir": "../",
    "*.js": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.scss": [
      "stylelint --syntax scss --fix"
    ]
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json"
    ],
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "setupFiles": [
      "./src/test/setup.js"
    ],
    "globals": {
      "__CLIENT__": false,
      "__SERVER__": true
    }
  },
