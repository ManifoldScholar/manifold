{
  "name": "manifold-client",
  "description": "The Manifold Scholarship Client",
  "author": "Zach Davis <zach@castironcoding.com>",
  "license": "GPL-3.0",
  "version": "0.0.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/manifoldScholar/manifold"
  },
  "keywords": [],
  "main": "bin/server.js",
  "scripts": {
    "clean": "rimraf -r tmp/dist && rimraf -r dist",
    "build": "npm-run-all build:prod",
    "build:prod": "NODE_ENV=production npm-run-all _build",
    "build:dev": "NODE_ENV=development npm-run-all _build",
    "start": "npm-run-all _serve:ssr",
    "watch": "npm-run-all clean _watch:banner --parallel _watch:ssr _watch:browser _build:ssr:rescue _serve:ssr:rescue",
    "test": "jest --verbose",
    "test-ci": "jest --runInBand",
    "fix": "npm-run-all \"fix:js {*}\" \"fix:scss {*}\" --",
    "lint": "npm-run-all \"lint:js {*}\" \"lint:scss {*}\" --",
    "fix:js": "./script/lint-javascript --fix",
    "fix:scss": "./script/lint-sass --fix",
    "fix:staged": "yarn lint-staged",
    "lint:js": "./script/lint-javascript",
    "lint:scss": "./script/lint-sass",
    "_build": "npm-run-all clean --parallel _build:browser _build:ssr",
    "_build:browser": "IS_BUILD=true node -r @babel/register node_modules/.bin/webpack --colors --optimize-minimize --config ./webpack/config/browser-prd.config.js",
    "_build:ssr": "IS_BUILD=true node -r @babel/register node_modules/.bin/webpack --color --optimize-minimize --config ./webpack/config/ssr-prd.config.js",
    "_build:ssr:rescue": "IS_BUILD=true node -r @babel/register node_modules/.bin/webpack --color --optimize-minimize --config ./webpack/config/ssr-rescue.config.js",
    "_serve:ssr": "node dist/manifold/ssr/manifold-client-ssr.js",
    "_serve:ssr:rescue": "yarn _wait:builds && npm-run-all --parallel _serve:ssr:rescue:server _serve:ssr:rescue:render",
    "_serve:ssr:rescue:server": "node dist/manifold/ssr/manifold-client-ssr-rescue.js",
    "_serve:ssr:rescue:render": "SSR_RESCUE=TRUE nodemon -q dist/manifold/ssr/manifold-client-ssr.js --watch dist/manifold/ssr/manifold-client-ssr.js",
    "_watch:banner": "node ./script/banner.js",
    "_watch:ssr": "IS_BUILD=true node -r @babel/register node_modules/.bin/webpack --color --watch --config ./webpack/config/ssr-dev.config.js",
    "_watch:browser": "node -r @babel/register webpack/servers/webpack-dev-server",
    "_wait:builds": "node script/wait.js --name=\"SSR\" --base=dist/manifold/ --on=ssr/manifold-client-ssr-rescue.js,ssr/manifold-client-ssr.js,manifest.json"
  },
  "lint-staged": {
    "relative": true,
    "linters": {
      "*.js": "yarn fix",
      "*.scss": "yarn fix"
    }
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json"
    ],
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "modulePaths": [
      "node_modules",
      "src"
    ],
    "setupFiles": [
      "./src/test/setup.js"
    ],
    "modulePathIgnorePatterns": [
      "<rootDir>/.*/__mocks__"
    ],
    "globals": {
      "__BROWSER__": false,
      "__SERVER__": true
    }
  },
  "dependencies": {
    "actioncable": "~5.0.2",
    "autolinker": "^1.2.1",
    "autoprefixer": "^9.0",
    "babel-polyfill": "^6.23.0",
    "brackets2dots": "^1.1.0",
    "classnames": "^2.2.1",
